{
  "version": 2,
  "name": "fraudguard-webapp",
  "builds": [
    {
      "src": "WebApp/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "WebApp/wsgi.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production",
    "PYTHONUNBUFFERED": "true",
    "VERCEL_PYTHON_VERSION": "3.11"
  },
  "regions": ["iad1"],
  "functions": {
    "WebApp/wsgi.py": {
      "maxDuration": 30
    }
  }
}
